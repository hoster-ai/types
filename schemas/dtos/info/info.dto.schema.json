{
  "$ref": "#/definitions/InfoDto",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "ActionDto": {
      "additionalProperties": false,
      "properties": {
        "icon": {
          "description": "The icon of the action.",
          "type": "string"
        },
        "label": {
          "description": "The label of the action (optional).",
          "type": "string"
        },
        "openMethod": {
          "enum": [
            "ajax_call",
            "large_iframe",
            "medium_iframe",
            "small_iframe"
          ],
          "type": "string"
        },
        "url": {
          "description": "The link of the action.",
          "type": "string"
        }
      },
      "required": [
        "icon",
        "openMethod",
        "url"
      ],
      "type": "object"
    },
    "FieldDto": {
      "additionalProperties": false,
      "properties": {
        "disabled": {
          "description": "Indicates if the field is disabled",
          "type": "boolean"
        },
        "hidden": {
          "description": "Indicates if the field is hidden",
          "type": "boolean"
        },
        "id": {
          "description": "ID of action field",
          "type": "string"
        },
        "label": {
          "description": "Label of action field",
          "items": {
            "$ref": "#/definitions/MultilangTextDto"
          },
          "type": "array"
        },
        "regexValidation": {
          "description": "Regex validation pattern for the field",
          "type": "string"
        },
        "regexValidationErrorMessage": {
          "description": "Error message for the field for supported languages",
          "items": {
            "$ref": "#/definitions/MultilangTextDto"
          },
          "type": "array"
        },
        "remoteValidation": {
          "description": "Indicates if the field has remote validation",
          "type": "boolean"
        },
        "remoteValidationErrorMessage": {
          "description": "Error message for the field",
          "items": {
            "$ref": "#/definitions/MultilangTextDto"
          },
          "type": "array"
        },
        "required": {
          "description": "Indicates if the field is required",
          "type": "boolean"
        },
        "type": {
          "$ref": "#/definitions/FieldTypeEnum",
          "description": "Type of label"
        },
        "value": {
          "anyOf": [
            {
              "$ref": "#/definitions/Record"
            },
            {
              "type": [
                "string",
                "number"
              ]
            }
          ],
          "description": "Value of action field"
        }
      },
      "required": [
        "disabled",
        "hidden",
        "id",
        "label",
        "required",
        "type",
        "value"
      ],
      "type": "object"
    },
    "InfoDto": {
      "additionalProperties": false,
      "description": "DTO for integration information\nContains all information related to a service integration",
      "properties": {
        "adminPanel": {
          "additionalProperties": false,
          "properties": {
            "actions": {
              "additionalProperties": false,
              "properties": {
                "client": {
                  "items": {
                    "$ref": "#/definitions/ActionDto"
                  },
                  "type": "array"
                },
                "item": {
                  "items": {
                    "$ref": "#/definitions/ActionDto"
                  },
                  "type": "array"
                }
              },
              "type": "object"
            },
            "menu": {
              "$ref": "#/definitions/MenuDto"
            },
            "productTabs": {
              "items": {
                "$ref": "#/definitions/TabDto"
              },
              "type": "array"
            },
            "settings": {
              "$ref": "#/definitions/MenuDto",
              "description": "Option that will appear in the \"Settings\" section (optional)"
            }
          },
          "type": "object"
        },
        "clientPanel": {
          "additionalProperties": false,
          "properties": {
            "actions": {
              "additionalProperties": false,
              "properties": {
                "item": {
                  "items": {
                    "$ref": "#/definitions/ActionDto"
                  },
                  "type": "array"
                }
              },
              "type": "object"
            },
            "menu": {
              "$ref": "#/definitions/MenuDto"
            },
            "productTabs": {
              "items": {
                "$ref": "#/definitions/TabDto"
              },
              "type": "array"
            }
          },
          "type": "object"
        },
        "description": {
          "description": "Description of the integration and its services (optional)",
          "type": "string"
        },
        "item_attributes": {
          "items": {
            "$ref": "#/definitions/FieldDto"
          },
          "type": "array"
        },
        "listen_events": {
          "items": {
            "$ref": "#/definitions/EventsEnum"
          },
          "type": "array"
        },
        "logo": {
          "description": "The logo of the integration (optional)",
          "type": "string"
        },
        "onboardingUrl": {
          "description": "The url for the onboarding process after installation of the integration",
          "type": "string"
        },
        "payPerUseUnits": {
          "items": {
            "$ref": "#/definitions/UnitDto"
          },
          "type": "array"
        },
        "product_attributes": {
          "items": {
            "$ref": "#/definitions/FieldDto"
          },
          "type": "array"
        },
        "requiredRoles": {
          "description": "The roles that need to be accepted by the company",
          "items": {
            "$ref": "#/definitions/RolesEnum"
          },
          "type": "array"
        },
        "responseDataFieldNames": {
          "$ref": "#/definitions/Record_1"
        },
        "supported_languages": {
          "items": {
            "$ref": "#/definitions/LanguageEnum"
          },
          "type": "array"
        },
        "title": {
          "description": "The title of the integration",
          "type": "string"
        },
        "unsupportedActions": {
          "default": [],
          "items": {
            "$ref": "#/definitions/ActionsEnum"
          },
          "type": "array"
        }
      },
      "required": [
        "supported_languages",
        "title",
        "unsupportedActions"
      ],
      "type": "object"
    },
    "MenuDto": {
      "$ref": "#/definitions/MenuDto_1",
      "description": "DTO for menu items\nExtends TabDto and adds icon information"
    },
    "MenuDto_1": {
      "additionalProperties": false,
      "description": "DTO for menu items\nExtends TabDto and adds icon information",
      "properties": {
        "icon": {
          "description": "The icon to be displayed for the menu item",
          "type": "string"
        },
        "label": {
          "description": "The name to be displayed for the menu item",
          "type": "string"
        },
        "tabs": {
          "description": "The list of tabs that will appear in the submenu and as a navigation bar above the main content\nIn case of only one tab, there will be neither a submenu nor a navigation bar.",
          "items": {
            "$ref": "#/definitions/TabDto"
          },
          "type": "array"
        }
      },
      "required": [
        "icon",
        "label",
        "tabs"
      ],
      "type": "object"
    },
    "MultilangTextDto": {
      "additionalProperties": false,
      "properties": {
        "language": {
          "$ref": "#/definitions/LanguageEnum"
        },
        "text": {
          "type": "string"
        }
      },
      "required": [
        "language",
        "text"
      ],
      "type": "object"
    },
    "Record": {
      "$ref": "#/definitions/__type"
    },
    "Record_1": {
      "$ref": "#/definitions/Record_2"
    },
    "Record_2": {
      "$ref": "#/definitions/__type_1"
    },
    "TabDto": {
      "additionalProperties": false,
      "description": "DTO for tabs\nUsed for defining tabs in the user interface",
      "properties": {
        "label": {
          "description": "The label displayed on the tab",
          "type": "string"
        },
        "url": {
          "description": "The URL associated with the tab.\nThe requests coming from the hoster will be signed\nwith jwt, which will contain information about the company",
          "type": "string"
        }
      },
      "required": [
        "label",
        "url"
      ],
      "type": "object"
    },
    "UnitDto": {
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "intervalDescription": {
          "type": "string"
        },
        "unitDescription": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "intervalDescription",
        "unitDescription"
      ],
      "type": "object"
    },
    "__type": {
      "additionalProperties": false,
      "type": "object"
    },
    "__type_1": {
      "additionalProperties": false,
      "patternProperties": {
        "^[0-9]+$": {
          "type": "string"
        }
      },
      "type": "object"
    }
  }
}